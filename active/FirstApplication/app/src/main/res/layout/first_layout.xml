<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/button1"
        android:text="Totast Test"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <Button
        android:id="@+id/button2"
        android:text="Finish"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/btn_showActive"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Activity Test" />
    <Button
        android:id="@+id/btn_steatchActive"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="隐式Intent Test" />

    <Button
        android:id="@+id/btn_brower"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="隐式Intent浏览器Test" />
    <Button
        android:id="@+id/btn_datapush"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Intent Data Push" />

    <Button
        android:id="@+id/btn_connectPrinter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="连接打印机" />


</LinearLayout>